<script>
    $(function () {


        $(".Lx_menu .at").each(function (index) {
            $(this).click(function () {
                // to do something
                // index计数
                var $Xzs = $(this).index();

                $(this).find(".Xz").addClass("addXz").parent().siblings().children().removeClass("addXz");
                $(".Lx_menu_con").css("display", "block");
                $(".Lx_menu_con").children().eq($Xzs).show().siblings().hide();
            });

        });

        $(".Close_gb").click(function () {
            $(".Lx_menu_con").css("display", "none");
            $(".Lx_menu .at").find(".Xz").removeClass("addXz");
        });
        $(".Db_Address_right .Address_right_SQ").eq(0).show().siblings().hide();
        $(".Db_Addrees_left ul li").eq(0).addClass("Address_bj");
        $(".Db_Addrees_left ul li").click(function () {

            var $lis = $(this).index();
            $(".Db_Img").hide().siblings().show();
            $(this).parent().parent().siblings().children().eq($lis).show().siblings().hide();
            $(this).addClass("Address_bj").siblings().removeClass("Address_bj");

        });

    });


    mui.init();
    mui.ready(function () {
        var header = document.querySelector('header.mui-bar');
        var list = document.getElementById('list');
        //calc hieght
        list.style.height = (document.body.offsetHeight - header.offsetHeight) + 'px';
        //create
        window.indexedList = new mui.IndexedList(list);
    });
    $(window).scroll(function () {
        //为页面添加页面滚动监听事件
        var wst = $(window).scrollTop() //滚动条距离顶端值
        $(".userlist .lab").each(function () {
            if ($(this).offset().top - 50 <= wst) { //判断滚动条位置 减去距离顶部的高度
                $(this).addClass("active").siblings().removeClass("active");
                $(".Yw li:contains('" + $(this).text() + "')").addClass("active").siblings().removeClass("active");
            }
        })

    })
    $(".mui-indexed-list").height($(window).height() - 10);
    document.getElementById("Qk").onclick = function () {
        document.getElementById("txt").value = "";

    }
    mui('body').on('tap', 'a', function () { document.location.href = this.href; });
</script>
