<script>
   
    $(window).scroll(function () {
        //为页面添加页面滚动监听事件
        var wst = $(window).scrollTop() //滚动条距离顶端值 
        $(".userlist .lab").each(function () {
            if ($(this).offset().top-50 <= wst) { //判断滚动条位置 减去距离顶部的高度
                $(this).addClass("active").siblings().removeClass("active");
                $(".Yw li:contains('" + $(this).text()+"')").addClass("active").siblings().removeClass("active");
            } 
        })

    })
</script>
