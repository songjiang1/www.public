<script>
    $(function () {
        //GetRspresentBindList("");
        GetOrganizeListJson();
    })
    
    function GetOrganizeListJson() {

        $.ajax({
            //async: false,
            type: "post",
            url: "/Organize/GetListJson",
            data: {
                OrganizeId: "",
                ParentId: "0", 
            },
            success: function (d) {
                var obj = eval("(" + d + ")");
                var data = "";
                var html = '';
                if (obj.type == 1) {
                    data = eval("(" + obj.message + ")");
                    console.log(data);
                    if (data != null && data != "" && data.length > 0) {
                        for (var i = 0; i < data.length; i++) {
                            html += ("<li onclick=\"GetOrganize(\'" + data[i].OrganizeId + "\',this)\">" + data[i].FullName+"</li>");  
                        }
                       
                    }
                } 
                $(".Db_Addrees_leftul").html(html);
                setTimeout(function () {
                    $(".Db_Addrees_leftul li").eq(0).click();
                },100)
            },
            error: function (xhr, type) {  
            }
        });
    }
    
    function GetOrganize(ParentId, thiz) {
        $(thiz).addClass("Address_bj").siblings().removeClass("Address_bj");
        var PId = ParentId;
        $.ajax({
            //async: false,
            type: "post",
            url: "/Organize/GetTreeListJson",
            data: { 
                ParentId: ParentId,
            },
            success: function (d, ParentId) {
                var obj = eval("(" + d + ")");
                var data = "";
                var html = '';
                if (obj.type == 1) {
                    data = eval("(" + obj.message + ")");
                    if (data != null && data != "" && data.length > 0) {
                        for (var i = 0; i < data.length; i++) {
                            var ifo = eval("(" + data[i].entityJson + ")");
                            if (!data[i].hasChildren) {
                                html += ("<li onclick=\"GeRspresentAddressList(\'" + data[i].id + "\')\">" + ifo.FullName + "</li>"); 
                            }
                            else if(data[i].hasChildren && ifo.ParentId==PId) {  
                                html += ("<li onclick=\"GetOrganize(\'" + data[i].id + "\',this)\">" + ifo.FullName + "</li>");  
                            } else {
                                html += ("<li onclick=\"GeRspresentAddressList(\'" + data[i].id + "\')\">" + ifo.FullName + "</li>");  
                            }
                            
                        } 
                      
                    }
                   
                }
                $(".Address_right_SQUL").html(html);
            },
            error: function (xhr, type) {
            }
        });
    }
     
    function GeRspresentAddressList(OrganizeId,thiz) {
        $(".Db_xq").hide().siblings().show();
        OrganizeId = OrganizeId == undefined ? "" : OrganizeId;
        $.ajax({
            //async: false,
            type: "get",
            url: "/Home/GeRspresentAddressList",
            data: {
                ParentId: "",
                TVShowMenuId: "Shuanglianxi_Root_page",
                OrganizeId: OrganizeId,
                Title: "",
                UserId:"",
            },
            success: function (d) {
                var obj = eval("(" + d + ")");
                var data = ""; 
                var html = '';
                if (obj.type == 1) {
                    data = eval("(" + obj.message + ")");
                    if (data != "" && data.length > 0) { 
                        for (var i = 0; i < data.length; i++) {
                            html += ('<div class="Db_Img_k">');
                            html += ('<img src="' + data[i].headicon+'"/>');
                            html += ("<span><a href=\"/Home/RepresentDetail?Id=" + data[i].id + "\">" + data[i].title+"</a></span>");
                            html += (' </div>');   
                        }
                    }
                    else {
                        html += ('暂无数据'); 
                    }
                }
                $(".Db_Img").html(html);
            },
            error: function (xhr, type) {
            }
        });
    }
